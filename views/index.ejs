                                                            <img src="img/cd/<%= viewModel.portfolio.lancamentos[i].arquivoCapaAlbum %>" alt="" class="imagemCD" />
                                                        </li>

                                                    <% } %>
                                                    
                                                </ul>
                                            </div>
                                        </article>
                                        <article id="slide_outros_trabalhos" class="slide" data-slide-left-position="-860">
