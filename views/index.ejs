                                                        <li class="song marginBottomCD"
                                                            data-h5a-song-artist="<%= viewModel.portfolio.lancamentos[i].artista %>"
                                                            data-h5a-song-name="<%= viewModel.portfolio.lancamentos[i].nome %>"
                                                            data-h5a-song-url="musicas/<%= viewModel.portfolio.lancamentos[i].arquivoMusica %>"
                                                            data-h5a-cover-url="img/cd/<%= viewModel.portfolio.lancamentos[i].arquivoCapaAlbum %>">
                                                            <figure class="play" >
                                                                <img src="img/play.png" alt="" />                                                        
                                                            </figure>
                                                            <figure class="pause" >
